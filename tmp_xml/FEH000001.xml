<?xml version="1.0" encoding="UTF-8"?>
<Invoice
  xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2"
  xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
  xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
  xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2"
  xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
  xmlns:xades="http://uri.etsi.org/01903/v1.3.2#"
  xmlns:sts="dian:gov:co:facturaelectronica:Structures-2-1"
>
  <!--
    ext:UBLExtensions: aquí va la firma XAdES exigida por DIAN.
    De momento va vacío; la firma la insertas luego con tu módulo de firma.
  -->
  <ext:UBLExtensions>
    <ext:UBLExtension>
      <ext:ExtensionContent>
        <!-- Firma XAdES-BES DIAN se inserta aquí (ds:Signature...) -->
      </ext:ExtensionContent>
    </ext:UBLExtension>
  </ext:UBLExtensions>

  <!--
    CustomizationID / ProfileExecutionID según DIAN:
    - CustomizationID: indica versión de anexo técnico
    - ProfileID: perfil del documento
    - ProfileExecutionID: 1 = producción, 2 = pruebas/habilitación
  -->
  <cbc:UBLVersionID>UBL 2.1</cbc:UBLVersionID>
  <cbc:CustomizationID>10</cbc:CustomizationID>
  <cbc:ProfileID>DIAN 2.1:FacturaElectronica</cbc:ProfileID>
  <cbc:ProfileExecutionID>2</cbc:ProfileExecutionID>

  <!--
    ID: número de la factura (prefijo + consecutivo)
    Ej: FEH1, FEH000001, etc.
  -->
  <cbc:ID>FEH000001</cbc:ID>

  <!-- UUID: CUFE con esquema de DIAN -->
  <cbc:UUID schemeID="1" schemeName="CUFE-SHA384">5AA9A8DC940FF8B0D6221B6B413578F6BEB6F30F96CE743DC19705E6EC1DCBC4BEAEFBF0189F60473B0A13BB606527A1</cbc:UUID>

  <cbc:IssueDate>2025-12-16</cbc:IssueDate>
  <cbc:IssueTime>14:30:00-05:00</cbc:IssueTime>

  <!-- InvoiceTypeCode: 01 = Factura electrónica de venta nacional -->
  <cbc:InvoiceTypeCode>01</cbc:InvoiceTypeCode>

  <!-- Moneda -->
  <cbc:DocumentCurrencyCode>COP</cbc:DocumentCurrencyCode>
  <cbc:LineCountNumeric>1</cbc:LineCountNumeric>

  <!-- Datos del Software DIAN -->
  <cac:Signature>
    <cbc:ID>00000000-0000-0000-0000-000000000000</cbc:ID>
    <cac:SignatoryParty>
      <cac:PartyIdentification>
        <cbc:ID>900123456</cbc:ID>
      </cac:PartyIdentification>
      <cac:PartyName>
        <cbc:Name>SALON DEMO</cbc:Name>
      </cac:PartyName>
    </cac:SignatoryParty>
    <cac:DigitalSignatureAttachment>
      <cac:ExternalReference>
        <cbc:URI>#SignatureDIAN</cbc:URI>
      </cac:ExternalReference>
    </cac:DigitalSignatureAttachment>
  </cac:Signature>

  <!-- Emisor (salón) -->
  <cac:AccountingSupplierParty>
    <cbc:AdditionalAccountID>48</cbc:AdditionalAccountID>
    <cac:Party>
      <cac:PartyIdentification>
        <cbc:ID schemeID="1" schemeName="NIT">900123456</cbc:ID>
      </cac:PartyIdentification>
      <cac:PartyName>
        <cbc:Name>SALON DEMO</cbc:Name>
      </cac:PartyName>
      <cac:PartyTaxScheme>
        <cbc:RegistrationName>SALON DEMO</cbc:RegistrationName>
        <cbc:CompanyID schemeID="1" schemeName="NIT">900123456</cbc:CompanyID>
        <cbc:TaxLevelCode>48</cbc:TaxLevelCode>
        <cac:TaxScheme>
          <cbc:ID>01</cbc:ID>
          <cbc:Name>IVA</cbc:Name>
        </cac:TaxScheme>
      </cac:PartyTaxScheme>
      <cac:PartyLegalEntity>
        <cbc:RegistrationName>SALON DEMO</cbc:RegistrationName>
      </cac:PartyLegalEntity>
      <cac:Contact>
        <cbc:ElectronicMail></cbc:ElectronicMail>
        <cbc:Telephone></cbc:Telephone>
      </cac:Contact>
      <cac:PhysicalLocation>
        <cac:Address>
          <cbc:ID></cbc:ID>
          <cbc:CityName></cbc:CityName>
          <cbc:CountrySubentity></cbc:CountrySubentity>
          <cac:AddressLine>
            <cbc:Line></cbc:Line>
          </cac:AddressLine>
          <cac:Country>
            <cbc:IdentificationCode>CO</cbc:IdentificationCode>
          </cac:Country>
        </cac:Address>
      </cac:PhysicalLocation>
    </cac:Party>
  </cac:AccountingSupplierParty>

  <!-- Adquiriente (cliente) -->
  <cac:AccountingCustomerParty>
    <cbc:AdditionalAccountID>13</cbc:AdditionalAccountID>
    <cac:Party>
      <cac:PartyIdentification>
        <cbc:ID schemeID="0" schemeName="CC">
          123456789
        </cbc:ID>
      </cac:PartyIdentification>
      <cac:PartyName>
        <cbc:Name>Cliente Prueba</cbc:Name>
      </cac:PartyName>
      <cac:PartyTaxScheme>
        <cbc:RegistrationName>Cliente Prueba</cbc:RegistrationName>
        <cbc:CompanyID>123456789</cbc:CompanyID>
        <cac:TaxScheme>
          <cbc:ID>01</cbc:ID>
          <cbc:Name>IVA</cbc:Name>
        </cac:TaxScheme>
      </cac:PartyTaxScheme>
      <cac:PhysicalLocation>
        <cac:Address>
          <cbc:ID></cbc:ID>
          <cbc:CityName></cbc:CityName>
          <cbc:CountrySubentity></cbc:CountrySubentity>
          <cac:AddressLine>
            <cbc:Line></cbc:Line>
          </cac:AddressLine>
          <cac:Country>
            <cbc:IdentificationCode>CO</cbc:IdentificationCode>
          </cac:Country>
        </cac:Address>
      </cac:PhysicalLocation>
      <cac:Contact>
        <cbc:ElectronicMail>cliente@prueba.com</cbc:ElectronicMail>
        <cbc:Telephone></cbc:Telephone>
      </cac:Contact>
    </cac:Party>
  </cac:AccountingCustomerParty>

  <!-- Medios de pago -->
  <cac:PaymentMeans>
    <cbc:ID>1</cbc:ID>
    <cbc:PaymentMeansCode>10</cbc:PaymentMeansCode>
    <cbc:PaymentDueDate>2025-12-16</cbc:PaymentDueDate>
  </cac:PaymentMeans>

  <!-- Totales de impuestos -->
  <cac:TaxTotal>
    <cbc:TaxAmount currencyID="COP">5700.00</cbc:TaxAmount>
    <cac:TaxSubtotal>
      <cbc:TaxableAmount currencyID="COP">30000.00</cbc:TaxableAmount>
      <cbc:TaxAmount currencyID="COP">5700.00</cbc:TaxAmount>
      <cac:TaxCategory>
        <cac:TaxScheme>
          <cbc:ID>01</cbc:ID>
          <cbc:Name>IVA</cbc:Name>
        </cac:TaxScheme>
      </cac:TaxCategory>
    </cac:TaxSubtotal>
  </cac:TaxTotal>

  <!-- Totales monetarios -->
  <cac:LegalMonetaryTotal>
    <cbc:LineExtensionAmount currencyID="COP">30000.00</cbc:LineExtensionAmount>
    <cbc:TaxExclusiveAmount currencyID="COP">30000.00</cbc:TaxExclusiveAmount>
    <cbc:TaxInclusiveAmount currencyID="COP">35700.00</cbc:TaxInclusiveAmount>
    <cbc:PayableAmount currencyID="COP">35700.00</cbc:PayableAmount>
  </cac:LegalMonetaryTotal>

  <!-- Referencia a resolución DIAN -->
  <cac:AdditionalDocumentReference>
    <cbc:ID>123456789012345</cbc:ID>
    <cbc:IssueDate>2025-01-15</cbc:IssueDate>
    <cbc:DocumentType>Resolution</cbc:DocumentType>
    <cbc:DocumentDescription>
      Prefijo FEH desde 1 hasta 999999999, vigencia 2025-02-01 a 2027-01-31
    </cbc:DocumentDescription>
  </cac:AdditionalDocumentReference>

  
  <cac:InvoiceLine>
    <cbc:ID>1</cbc:ID>
    <cbc:InvoicedQuantity unitCode="NIU">1</cbc:InvoicedQuantity>
    <cbc:LineExtensionAmount currencyID="COP">30000.00</cbc:LineExtensionAmount>
    <cac:TaxTotal>
      <cbc:TaxAmount currencyID="COP">5700.00</cbc:TaxAmount>
      <cac:TaxSubtotal>
        <cbc:TaxableAmount currencyID="COP">30000.00</cbc:TaxableAmount>
        <cbc:TaxAmount currencyID="COP">5700.00</cbc:TaxAmount>
        <cac:TaxCategory>
          <cbc:Percent>19</cbc:Percent>
          <cbc:TaxExemptionReasonCode>S</cbc:TaxExemptionReasonCode>
          <cac:TaxScheme>
            <cbc:ID>01</cbc:ID>
            <cbc:Name>IVA</cbc:Name>
          </cac:TaxScheme>
        </cac:TaxCategory>
      </cac:TaxSubtotal>
    </cac:TaxTotal>
    <cac:Item>
      <cbc:Description>Corte de cabello</cbc:Description>
    </cac:Item>
    <cac:Price>
      <cbc:PriceAmount currencyID="COP">30000.00</cbc:PriceAmount>
      <cbc:BaseQuantity unitCode="NIU">1</cbc:BaseQuantity>
    </cac:Price>
  </cac:InvoiceLine>
</Invoice>
